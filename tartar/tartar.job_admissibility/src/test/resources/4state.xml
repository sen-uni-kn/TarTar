<nta>
	<declaration>// Place global declarations here.
bool b = true;
clock x, y;</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-425" y="-187">
		</location>
		<location id="id1" x="-348" y="-119">
			<name x="-331" y="-119">Loc2</name>
		</location>
		<location id="id2" x="-348" y="-187">
			<name x="-331" y="-187">Loc1</name>
			<label kind="invariant" x="-289" y="-187">y &lt;=1</label>
		</location>
		<location id="id3" x="-348" y="-246">
			<name x="-331" y="-246">Loc0</name>
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id3"/>
			<target ref="id2"/>
			<label kind="guard" x="-408" y="-238">x &gt; 3</label>
			<label kind="assignment" x="-416" y="-221">y:=0</label>
			<nail x="-382" y="-212"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="guard" x="-399" y="-187">x==2</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="-331" y="-161">x==1</label>
			<label kind="assignment" x="-289" y="-161">b= false</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id2"/>
			<label kind="guard" x="-314" y="-229">x&lt;=1</label>
			<label kind="assignment" x="-314" y="-212">y:=0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula>A[]b
			</formula>
			<comment>
			</comment>
		</query>
	</queries>
</nta>
